generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
// user model query 
model User {
  id Int @id @default(autoincrement())
  name String
  email String @unique
  lastName String
  UserName String @unique
  role String
  
}